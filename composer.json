{
  "name": "bengbengsoft/beng-admin",
  "description": "RBAC Auth manager for Yii2 ",
  "keywords": ["yii2", "admin", "auth", "rbac"],
  "type": "yii2-extension",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/hahastein/beng-admin/issues",
    "source": "https://github.com/hahastein/beng-admin"
  },
  "authors": [
    {
      "name": "hahastein",
      "email": "146119@qq.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "yiisoft/yii2": "~2.0.6",
    "yiisoft/yii2-bootstrap": "~2.0.0",
    "bengbengsoft/beng-framework": "^0.9.1",
    "bengbengsoft/beng-tools-upload": "^0.9.0",
    "upyun/sdk": "dev-master"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "bengbeng\\admin\\": ""
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
